# general plugin information
plugin.com.bluemarsh.jswat.plugins.jedit.JSwatPlugin.activate=defer
plugin.com.bluemarsh.jswat.plugins.jedit.JSwatPlugin.name=JSwatPlugin
plugin.com.bluemarsh.jswat.plugins.jedit.JSwatPlugin.author=Nathan Fiedler
plugin.com.bluemarsh.jswat.plugins.jedit.JSwatPlugin.version=1.6
plugin.com.bluemarsh.jswat.plugins.jedit.JSwatPlugin.docs=README.html
plugin.com.bluemarsh.jswat.plugins.jedit.JSwatPlugin.depend.0=jdk 1.4
plugin.com.bluemarsh.jswat.plugins.jedit.JSwatPlugin.depend.1=jedit 04.02.11.00
plugin.com.bluemarsh.jswat.plugins.jedit.JSwatPlugin.depend.2=optional plugin archive.ArchivePlugin 0.4
plugin.com.bluemarsh.jswat.plugins.jedit.JSwatPlugin.jars=jswat.jar \
        jswat-parser.jar \
        jclasslib.jar

# dockable window name
jswatplugin.title=JSwat

# application menu items
plugin.com.bluemarsh.jswat.plugins.jedit.JSwatPlugin.menu.label=JSwat
plugin.com.bluemarsh.jswat.plugins.jedit.JSwatPlugin.menu=\
