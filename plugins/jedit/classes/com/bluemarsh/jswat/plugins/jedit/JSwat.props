        jswatplugin.vmStart \
        jswatplugin.vmStop \
        jswatplugin.vmAttach \
        jswatplugin.vmDetach \
        - \
        jswatplugin.vmSuspend \
        jswatplugin.vmResume \
        - \
        jswatplugin.toggleBreakpoint \
        - \
        jswatplugin.stepInto \
        jswatplugin.stepOver \
        jswatplugin.stepOut \
        jswatplugin.runUntil \
        - \
        jswatplugin.hotswapClass \
        - \
        jswatplugin.helpIndex \
        jswatplugin.showOptions

jswatplugin.label=Show JSwat Plugin
jswatplugin.vmStart.label=Start VM
jswatplugin.vmStop.label=Stop VM
jswatplugin.vmAttach.label=Attach to remote VM
jswatplugin.vmDetach.label=Detach from remote VM
jswatplugin.vmSuspend.label=Suspend VM
jswatplugin.vmResume.label=Resume VM
jswatplugin.toggleBreakpoint.label=Toggle Breakpoint
jswatplugin.stepInto.label=Step Into
jswatplugin.stepOver.label=Step Over
jswatplugin.stepOut.label=Step Out
jswatplugin.runUntil.label=Run Until
jswatplugin.hotswapClass.label=Hotswap Class
jswatplugin.helpIndex.label=Help...
jswatplugin.showOptions.label=Show Options...

# Option pane
plugin.com.bluemarsh.jswat.plugins.jedit.JSwatPlugin.option-pane=jswatplugin

# Option pane labels
options.jswatplugin.label=JSwat
options.jswatplugin.code=new com.bluemarsh.jswat.plugins.jedit.JSwatOptionPane()

options.jswatplugin.classpath.title=Class Path:
options.jswatplugin.sourcepath.title=Source Path:
options.jswatplugin.otherOptions.title=Options:
options.jswatplugin.stopOnMain.title=Stop in main() method when launching
options.jswatplugin.raiseWindow.title=Raise window upon debugging event
options.jswatplugin.shortClassNames.title=Shorten class names

# Toolbar
options.jswatplugin.toolbar=vmAttach vmClose - vmStart vmStop \
        vmSuspend vmResume setBreak - step next finish

# Error dialogs
options.jswatplugin.error.invokeAction.title=JSwat Error
options.jswatplugin.error.invokeAction.message=\
        An error occured during execution of a JSwat action:\n\
        {0}\n\
        \n\
        For more information see Activity Log.

options.jswatplugin.error.invokeCommand.title=JSwat Error
options.jswatplugin.error.invokeCommand.message=\
        An error occured during execution of the following JSwat command:\n\
        {0}\n\
        \n\
        The error message was:\n\
        {1}.\n\
        \n\
        For more information see Activity Log.

options.jswatplugin.error.noCodeAtLine.title=JSwat Error
options.jswatplugin.error.noCodeAtLine.message=\
        No code at this line in this class.

# Miscelleanous
options.jswatplugin.error.onlyOneInstance.label=\
        <html><body>\
        <strong>Error: Another instance of the JSwatPlugin is already \
        running in another jEdit view.</strong><p>\
        <p> \
        In the current implementation only one instance of the JSwatPlugin \
        may be running.<p> \
        <p> \
        <u>Note</u>: If the first jEdit view has already been closed, try to \
        re-dock the JSwatPlugin to a different position. Invoke <em>Global \
        Options &gt; Docking</em> and choose a different dock position for \
        entry "JSwat". Close the dialog with "Ok".\
        </body></html>

